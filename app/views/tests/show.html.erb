<div class="test">
  <div class="title"><%= @test.title %> | Questions Count: <%= @test.questions.size %></div>
  <ol>
    <% @questions.each do |question| %>
      <li><%= question.body %> | <%= link_to '->', question_path(question) %></li>
    <% end %>
  </ol>
</div>

<%= link_to 'All tests', tests_path %> |
<%= link_to 'New question', new_test_question_path(@test) %>